<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>SampleDynamicWeb</display-name>

	<servlet>
		<servlet-name>ProcessorServlet</servlet-name>
		<servlet-class>com.bs.ProcessorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProcessorServlet</servlet-name>
		<url-pattern>/ProcessorServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>MakePaymentServlet</servlet-name>
		<servlet-class>com.bs.MakePaymentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MakePaymentServlet</servlet-name>
		<url-pattern>/MakePaymentServlet</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>Menu.jsp</welcome-file>
	</welcome-file-list>
</web-app>