--  Ref Constraints for Table DEPT_EMP
--------------------------------------------------------

  ALTER TABLE DEPT_EMP ADD FOREIGN KEY (EMPNO)
	  REFERENCES EMPLOYEES (EMP_NO) ;
  ALTER TABLE DEPT_EMP ADD FOREIGN KEY (DEPTNO)
	  REFERENCES DEPARTMENTS (DEPT_NO) ;
