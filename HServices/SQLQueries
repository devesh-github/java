--------------------------------------------------------

  CREATE TABLE DEPT_MANAGER 
   (	DMID DOUBLE, 
	EMPNUMBER DOUBLE, 
	DEPTNUMBER DOUBLE, 
	FROM_DATE DATE, 
	TO_DATE DATE
   ) ;
--------------------------------------------------------
--  DDL for Table EMPLOYEES
--------------------------------------------------------

  CREATE TABLE EMPLOYEES 
   (	EMP_NO DOUBLE, 
	BIRTH_DATE DATE, 
	FIRST_NAME VARCHAR(100 ), 
	LAST_NAME VARCHAR(100 ), 
	HIRE_DATE DATE, 
	TITLESNO DOUBLE
   ) ;
--------------------------------------------------------
--  DDL for Table SALARIES
--------------------------------------------------------

  CREATE TABLE SALARIES 
   (	SALID DOUBLE, 
	EMPNO DOUBLE, 
	SALARY DOUBLE, 
	FROM_DATE DATE, 
	TO_DATE DATE
   )  ;
--------------------------------------------------------
