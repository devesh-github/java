  CREATE TABLE DEPARTMENTS 
   (	DEPT_NO DOUBLE, 
	DEPT_NAME VARCHAR(200 )
   )  ;
--------------------------------------------------------
--  DDL for Table DEPT_EMP
--------------------------------------------------------

  CREATE TABLE DEPT_EMP 
   (	DEID DOUBLE, 
	EMPNO DOUBLE, 
	DEPTNO DOUBLE, 
	FROM_DATE DATE, 
	TO_DATE DATE
   ) ;
